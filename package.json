{
  "name": "middle.messenger.praktikum.yandex",
  "version": "1.0.0",
  "description": "simple messenger",
  "source": "static/index.html",
  "scripts": {
    "dev": "parcel --no-cache",
    "build": "node ./node_modules/.bin/webpack",
    "start": "npm run build && node ./server.js",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint-fix": "stylelint --fix .",
    "test": "ts-mocha src/**/*.spec.ts -require test/mock.js",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacksonrr3/middle.messenger.praktikum.yandex.git"
  },
  "author": "evgeniy_zaborshchikov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jacksonrr3/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/jacksonrr3/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "2.7.0",
    "@types/chai": "4.3.4",
    "@types/expect": "24.3.0",
    "@types/mocha": "10.0.0",
    "@types/node": "^18.11.9",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "chai": "4.3.7",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.7.2",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "html-webpack-plugin": "5.5.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "mini-css-extract-plugin": "^2.7.0",
    "mocha": "10.1.0",
    "npm-run-all": "^4.1.5",
    "parcel": "2.7.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "stylelint": "14.14.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "29.0.0",
    "stylelint-scss": "4.3.0",
    "ts-loader": "^9.4.1",
    "ts-mocha": "10.0.0",
    "typescript": "4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
