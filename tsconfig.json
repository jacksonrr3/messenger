{
	"compilerOptions": {
        "outDir": "dist",
        "target": "ESNext",
        "declaration": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "baseUrl": "./src",
        "moduleResolution": "Node",
    }
}
