{
	"compilerOptions": {
        "outDir": "dist",
        "target": "es2022",
        "declaration": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "noImplicitAny": false,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "sourceMap": true,
        "baseUrl": "./src",
        "moduleResolution": "Node16"
    }
}