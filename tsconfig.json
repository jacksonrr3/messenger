{
	"compilerOptions": {
        "outDir": "dist",
        "target": "es2022",
        "declaration": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "baseUrl": "./src",
        "moduleResolution": "Node",
        "lib": ["ES2020", "ES2017.Object"],
    }
}
